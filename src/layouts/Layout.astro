---
import '@fontsource/poppins/100.css';
import '@fontsource/poppins/200.css';
import '@fontsource/poppins/300.css';
import '@fontsource/poppins/400.css';
import '@fontsource/poppins/500.css';
import '@fontsource/poppins/600.css';
import '@fontsource/poppins/700.css';
import '@fontsource/poppins/800.css';
import '@fontsource/poppins/900.css';
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Coaching de Felipe Pollitzer para alcanzar tus metas y vivir con propósito" />
		<meta name="keywords" content="Coaching, Coaching ontológico, Propósito, Metas, Liderazgo, Crecimiento personal" />
		<meta name="robots" content="index, follow" />
		<meta property="og:title" content="Coaching | Felipe Pollitzer" />
		<meta property="og:description" content="Desbloquea tu máximo potencial con sesiones de coaching personalizadas para alcanzar tus metas y vivir con propósito" />
		<meta property="og:image" content="/about-image.webp" />
		<meta property="og:url" content="https://felipepollitzer.com/">
		<meta property="og:type" content="website">
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<title>{title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>
<style is:global>
	html {
		font-family: 'Poppins', sans-serif;
		background: #fff;
		scroll-behavior: smooth;
		color: #33372c;
	}
</style>
